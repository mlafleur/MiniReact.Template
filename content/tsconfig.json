{
  "compileOnSave": false,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    //"sourceMap": true,
    //"module": "es2015",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "."

  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
